          <stringProp name="script">class Entity {
    def name
}

def randomizer(nameList) {
	def nameListSize = nameList.size()
	def r = new Random()

	10.times{
	    Entity entity = new Entity(
	        name: nameList.get(r.nextInt(nameListSize))
	    )
	    result = entity.name
	}
	return result
}

